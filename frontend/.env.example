# Frontend Environment Variables
# Copy this file to .env.local and update with your actual values

# API Configuration - REQUIRED for production
# Set these explicitly in each environment
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws

# Production example:
# VITE_API_BASE_URL=https://api.meetingmind.app
# VITE_WS_URL=wss://api.meetingmind.app/ws

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG=true

# Analytics (optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Feature Flags (optional overrides)
VITE_ENABLE_AI_INSIGHTS=true
VITE_ENABLE_REAL_TIME_TRANSCRIPTION=true
VITE_ENABLE_SENTIMENT_ANALYSIS=true